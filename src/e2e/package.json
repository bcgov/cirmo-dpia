{
	"name": "dpia-e2e",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"playwright-local": "playwright test",
		"playwright-local-ui": "playwright test --ui",
		"playwright-local-debug": "playwright test --debug",
		"playwright-dev": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.DEV_URL)\") playwright test",
		"playwright-dev-ui": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.DEV_URL)\") playwright test --ui",
		"playwright-dev-debug": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.DEV_URL)\") playwright test --debug",
		"playwright-test": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.TEST_URL)\") playwright test",
		"playwright-test-ui": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.TEST_URL)\") playwright test --ui",
		"playwright-test-debug": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.TEST_URL)\") playwright test --debug",
		"playwright-prod": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.PROD_URL)\") playwright test",
		"playwright-prod-ui": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.PROD_URL)\") playwright test --ui",
		"playwright-prod-debug": "PLAYWRIGHT_BASE_URL=$(node -e \"console.log(require('dotenv').config().parsed.PROD_URL)\") playwright test --debug"
	},
	"dependencies": {
		"dotenv": "16.3.1"
	},
	"devDependencies": {
		"@playwright/test": "1.39.0",
		"@types/node": "20.8.9"
	}
}
